<UserControl
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Yamed.Oms"
             xmlns:dxe="http://schemas.devexpress.com/winfx/2008/xaml/editors" x:Class="Yamed.Oms.ReestrChooseControl"
             mc:Ignorable="d" 
             d:DesignHeight="100" d:DesignWidth="250" Width="250" Height="100" MinWidth="260" MinHeight="110">
    <Grid Loaded="Grid_Loaded" Height="100" Width="250">
        <dxe:ButtonEdit HorizontalAlignment="Left" Margin="10,67,0,0" VerticalAlignment="Top" Width="230" DefaultButtonClick="ButtonEdit_OnDefaultButtonClick" Name="ButtonEdit" Height="23"/>
        <TextBlock x:Name="tbRCC" HorizontalAlignment="Left" Margin="10,10,0,0" TextWrapping="Wrap" TextAlignment="Center"  Text="Выберите реестр" VerticalAlignment="Top" Width="230" Height="19" FontFamily="Times New Roman" FontSize="14"/>
        <dxe:ComboBoxEdit x:Name="comboboxSchets" HorizontalAlignment="Left" Margin="10,34,0,0" VerticalAlignment="Top" Width="230" Height="28" DisplayMember="nameSchet" ValueMember="ID" SelectedIndexChanged="comboboxSchets_SelectedIndexChanged"/>
    </Grid>
</UserControl>